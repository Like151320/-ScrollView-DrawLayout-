<?xml version="1.0" encoding="utf-8"?>
<com.example.a26050.statusbaradapterdemo.measureTest.TScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".measureTest.MeasureTestActivity">

    <com.example.a26050.statusbaradapterdemo.measureTest.TFrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.example.a26050.statusbaradapterdemo.measureTest.TTextView
            android:layout_width="10dp"
            android:layout_height="1000dp"
            android:background="#F0F"
            android:visibility="gone" />

        <com.example.a26050.statusbaradapterdemo.measureTest.TWebView
            android:id="@+id/webView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

    </com.example.a26050.statusbaradapterdemo.measureTest.TFrameLayout>

</com.example.a26050.statusbaradapterdemo.measureTest.TScrollView>